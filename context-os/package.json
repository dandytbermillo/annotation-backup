{
  "name": "@context/os",
  "version": "1.0.0",
  "description": "Intelligent documentation orchestrator for compliant feature development",
  "main": "dist/index.js",
  "bin": {
    "context-os": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx index.ts",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepare": "npm run build",
    "context:status": "node cli/status-cli.js",
    "context:analyze": "node cli/analyze-cli.js"
  },
  "keywords": [
    "documentation",
    "orchestration",
    "compliance",
    "feature-development",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "fs-extra": "^11.1.0",
    "handlebars": "^4.7.8",
    "inquirer": "^9.2.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.0",
    "@types/inquirer": "^9.0.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "tsx": "^3.12.0",
    "typescript": "^5.0.0"
  },
  "directories": {
    "doc": "docs",
    "example": "example",
    "test": "test"
  },
  "types": "./dist/index.d.ts"
}
